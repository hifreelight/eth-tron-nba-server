{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Block": {
    "dataSource": "mongoDb",
    "public": false
  },
  "Rand": {
    "dataSource": null,
    "public": false
  },
  "Match": {
    "dataSource": "mongoDb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "_create": true,
          "basketball": true,
          "detail": true
        }
      }
    }
  },
  "Team": {
    "dataSource": "mongoDb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true
        }
      }
    }
  }
}
